{
  "private": true,
  "name": "kaw-testbed",
  "version": "0.1.0",
  "description": "Modern WordPress plugin environment for local R&D",
  "author": "K. Adam White",
  "license": "GPL-2.0-or-later",
  "browserslist": [
    "last 2 firefox versions",
    "last 2 chrome versions",
    "last 2 edge versions",
    "last 2 safari versions",
    "ie >= 11",
    "not ie < 10",
    ">1%"
  ],
  "scripts": {
    "build": "webpack --config=.config/webpack.config.prod.js",
    "start": "webpack-dev-server --config=.config/webpack.config.dev.js",
    "lint:js": "eslint --cache-location=.cache/eslint.json --ignore-path=.config/.eslintignore --ext js,jsx,ts,tsx .eslintrc.js .config src blocks",
    "lint:js:fix": "eslint --fix --cache-location=.cache/eslint.json --ignore-path=.config/.eslintignore --ext js,jsx,ts,tsx .eslintrc.js .config src blocks",
    "prelint:php": "mkdir -p .cache",
    "lint:php": "vendor/bin/phpcs --standard=.config/phpcs.xml -s --cache=.cache/phpcs.json inc blocks tests",
    "lint:php:fix": "make fix-php",
    "lint": "npm-run-all lint:*",
    "test:php": "vendor/bin/phpunit",
    "test:js": "jest",
    "test:js:watch": "jest --watch",
    "test:js:coverage": "jest --coverage",
    "test": "npm-run-all test:*"
  },
  "devDependencies": {
    "@humanmade/webpack-helpers": "^0.7.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^8.0.2",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.17",
    "@types/react-autosuggest": "^9.3.13",
    "@types/react-dom": "^16.9.4",
    "@types/webpack-env": "^1.14.1",
    "@types/wordpress__blocks": "^6.4.4",
    "@types/wordpress__components": "^8.5.2",
    "@types/wordpress__data": "^4.6.6",
    "@types/wordpress__data-controls": "^1.0.1",
    "@types/wordpress__editor": "^9.4.1",
    "@types/wordpress__element": "^2.4.0",
    "@types/wordpress__hooks": "^2.4.0",
    "@types/wordpress__notices": "^1.5.2",
    "@types/wordpress__rich-text": "^3.4.3",
    "@types/wordpress__shortcode": "^2.3.0",
    "@types/wordpress__url": "^2.3.0",
    "@wordpress/blocks": "6.12.0",
    "@wordpress/components": "9.2.1",
    "@wordpress/data": "4.14.0",
    "@wordpress/data-controls": "1.8.0",
    "@wordpress/editor": "9.12.1",
    "@wordpress/element": "2.11.0",
    "@wordpress/hooks": "2.7.0",
    "@wordpress/notices": "2.0.0",
    "@wordpress/rich-text": "3.12.0",
    "@wordpress/server-side-render": "1.8.1",
    "@wordpress/shortcode": "2.6.0",
    "@wordpress/url": "2.11.0",
    "block-editor-hmr": "^0.6.0",
    "eslint": "^6.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.4.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^24.2.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-autosuggest": "^9.4.3",
    "react-intersection-observer": "^8.25.3",
    "rememo": "^3.0.0"
  }
}
